{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "downlevelIteration": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "node",
      "jest",
      "@testing-library/jest-dom",
    ],
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "setupTest.ts"  ],
  "exclude": [
    "node_modules",
    ".eslintrc.json",
    "setupTest.ts",
    "__mocks__/handlers.tsx",
    "__mocks__/mswServer.tsx"
  ]
}
